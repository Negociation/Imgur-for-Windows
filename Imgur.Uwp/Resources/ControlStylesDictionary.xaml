                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <toolkit:DropShadowPanel x:Name="ButtonShadow" HorizontalContentAlignment="Stretch" OffsetX="0" BlurRadius="40"  OffsetY="6" ShadowOpacity="0" Color="#01b96b"  Visibility="Visible" HorizontalAlignment="Stretch">

                            <Grid x:Name="ButtonRoot"  Canvas.ZIndex="2"  HorizontalAlignment="Stretch" CornerRadius="6"  BorderThickness="2" BorderBrush="Transparent" Opacity="0.9">
                                <Rectangle x:Name="ButtonLayer" Opacity="0.2" RadiusX="6" RadiusY="6" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                                <ContentPresenter x:Name="ContentPresenter" AutomationProperties.AccessibilityView="Raw"  Windows10version1809:BackgroundSizing="{TemplateBinding BackgroundSizing}"  ContentTemplate="{TemplateBinding ContentTemplate}" Padding="0" Content="{TemplateBinding Content}" CornerRadius="6" ContentTransitions="{TemplateBinding ContentTransitions}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"></ContentPresenter>
                            </Grid>
                        </toolkit:DropShadowPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
