                                        <ToolTip x:Name="NavigationViewCloseButtonToolTip"/>
                                    </ToolTipService.ToolTip>
                                </Button>
                                <Button x:Name="TogglePaneButton"  HorizontalAlignment="Center" Background="Transparent" BorderBrush="Transparent" FocusVisualMargin="0"  AutomationProperties.LandmarkType="Navigation" MaxWidth="40" Height="40" VerticalAlignment="Top" Visibility="{Binding TemplateSettings.PaneToggleButtonVisibility, RelativeSource={RelativeSource Mode=TemplatedParent}}">
                                    <Button.Content>
                                        <FontIcon x:Name="PaneTitleTextBlock" FontFamily="{StaticResource SegoeMDLGlyphs}" Foreground="White" FontWeight="Light" FontSize="24" Glyph="&#xE700;"/>
